{
  "name": "background",
  "displayName": "background",
  "description": "Bring background images to your vscode",
  "version": "1.2.2-alpha.1",
  "preview": true,
  "scripts": {
    "vscode:uninstall": "node ./out/uninstall",
    "build": "gulp",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint . --ext .ts src",
    "fix": "eslint . --fix --ext .ts src",
    "package": "gulp package",
    "prepare": "husky install"
  },
  "publisher": "shalldie",
  "author": "shalldie",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shalldie/vscode-background.git"
  },
  "engines": {
    "vscode": "^1.40.0"
  },
  "icon": "vscode-background-logo.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.background.uninstall",
        "title": "%background.cmd.uninstall%",
        "shortTitle": "Background - Uninstall (remove extension)"
      },
      {
        "command": "extension.background.refresh",
        "title": "%background.cmd.refresh%",
        "shortTitle": "Background - Refresh"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%background.config.title%",
      "properties": {
        "background.enabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%background.config.enabled%"
        },
        "background.useFront": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%background.config.useFront%"
        },
        "background.useDefault": {
          "type": "boolean",
          "default": true,
          "description": "%background.config.useDefault%"
        },
        "background.style": {
          "type": "object",
          "default": {
            "content": "''",
            "pointer-events": "none",
            "position": "absolute",
            "z-index": "99999",
            "width": "100%",
            "height": "100%",
            "background-position": "100% 100%",
            "background-repeat": "no-repeat",
            "opacity": 1
          },
          "markdownDescription": "%background.config.style%"
        },
        "background.styles": {
          "type": "array",
          "default": [
            {},
            {},
            {}
          ],
          "markdownDescription": "%background.config.styles%"
        },
        "background.customImages": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "markdownDescription": "%background.config.customImages%"
        },
        "background.loop": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%background.config.loop%"
        },
        "background.editors.code": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%background.config.editors.code%"
        },
        "background.editors.empty": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "%background.config.editors.empty%"
        },
        "background.editors.started": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%background.config.editors.started%"
        },
        "background.editors.extension": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%background.config.editors.extension%"
        },
        "background.editors.difference": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%background.config.editors.difference%"
        },
        "background.editors.settings": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%background.config.editors.settings%"
        },
        "background.editors.fullscreen": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%background.config.editors.fullscreen%"
        },
        "background.opacities.decorationsOverviewRuler": {
          "type": "number",
          "default": 1,
          "maximum": 1,
          "minimum": 0,
          "markdownDescription": "%background.config.opacities.decorationsOverviewRuler%"
        },
        "background.lab.useVscodeFileUri": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%background.config.lab.useVscodeFileUri%"
        }
      }
    }
  },
  "devDependencies": {
    "@types/node": "^16.11.45",
    "@types/vscode": "^1.40.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "del": "^6.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "run-sequence": "^2.2.1",
    "typescript": "^4.7.4",
    "vsce": "^2.9.1",
    "vscode-nls-dev": "^4.0.3"
  },
  "dependencies": {
    "@vscode/sudo-prompt": "^9.3.1",
    "vscode-nls": "^5.2.0"
  }
}